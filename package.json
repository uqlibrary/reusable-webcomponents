{
  "name": "reusable-webcomponents",
  "version": "1.0.0",
  "description": "A pure javascript collection of ITS Design System components for UQ Library apps",
  "main": "src/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build:development": "cross-env NODE_ENV=development webpack && npm run compilesass:development",
    "build:production": "cross-env NODE_ENV=production webpack && npm run compilesass",
    "build:staging": "cross-env NODE_ENV=staging webpack && npm run compilesass",
    "build": "cross-env NODE_ENV=local webpack && npm run compilesass",
    "clean": "rm -rf node_modules dist cypress/screenshots cypress/video",
    "codestyles:fix:all": "cross-env prettier-eslint '**/*.js' --write",
    "codestyles:fix:staged": "cross-env FILES=$(git diff --cached --name-only --diff-filter=ACM | grep \".jsx\\{0,1\\}$\"); npx prettier-eslint $FILES --write",
    "codestyles:fix": "cross-env prettier-eslint --write",
    "compilesass:development": "node-sass ./src/applications --output-style compressed --output ./dist/$CI_BRANCH/applications",
    "compilesass": "node-sass ./src/applications --output ./dist/applications --output-style compressed",
    "cy:cache:list": "cypress cache list",
    "cy:info": "cypress info",
    "cy:open": "cypress open",
    "cy:run:record": "cypress run --record",
    "cy:run": "cypress run",
    "cy:verify": "cypress verify",
    "cy:version": "cypress version",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run --record",
    "eslint": "eslint '**/*.js' --quiet",
    "eslint:fix": "eslint '**/*.js' --quiet --fix",
    "start-test-server": "cross-env NODE_ENV=local USE_MOCK=true webpack-dev-server",
    "start:mock": "cross-env NODE_ENV=local USE_MOCK=true FULL_PATH=http://localhost:8080 NODE_OPTIONS='--max-old-space-size=2048' webpack-dev-server --open",
    "start": "cross-env NODE_ENV=local webpack-dev-server --open",
    "test:local": "start-server-and-test start-test-server http://localhost:8080 cypress:open",
    "test": "start-server-and-test start-test-server http://localhost:8080 cy:run"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/uqlibrary/reusable-webcomponents.git"
  },
  "author": "UQ Library",
  "license": "MIT",
  "homepage": "https://library.uq.edu.au",
  "devDependencies": {
    "axe-core": "^4.1.2",
    "babel-eslint": "^10.1.0",
    "clean-css-cli": "^5.2.2",
    "copy-webpack-plugin": "^6.2.1",
    "cypress": "^6.8.0",
    "cypress-axe": "^0.12.2",
    "cypress-plugin-tab": "^1.0.5",
    "eslint": "^7.22.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "file-loader": "^6.2.0",
    "mini-css-extract-plugin": "^1.4.1",
    "node-sass": "^4.14.1",
    "prettier": "2.2.1",
    "prettier-eslint-cli": "^5.0.1",
    "sass-loader": "^7.3.1",
    "start-server-and-test": "^1.12.0",
    "webpack": "^4.46.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.7.2"
  },
  "dependencies": {
    "autoprefixer": "^9.6.0",
    "@babel/core": "^7.5.0",
    "@babel/preset-env": "^7.13.9",
    "babel-loader": "^8.0.6",
    "content-replace-webpack-plugin": "^1.0.0",
    "copy-webpack-plugin": "^6.2.1",
    "cross-env": "^7.0.3",
    "css-loader": "^3.0.0",
    "cssnano": "^4.1.10",
    "core-js": "^3.9.1",
    "html-webpack-plugin": "^3.2.0",
    "js-cookie": "^2.2.1",
    "postcss-loader": "^3.0.0",
    "query-string": "^6.14.1",
    "regenerator-runtime": "^0.13.7",
    "replace-in-file-webpack-plugin": "^1.0.6",
    "style-loader": "^0.23.1",
    "terser-webpack-plugin": "^4.2.3"
  },
  "browserslist": [
    "defaults",
    "not IE 11",
    "maintained node versions"
  ]
}
