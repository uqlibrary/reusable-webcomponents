{
  "name": "reusable-webcomponents",
  "version": "1.0.0",
  "description": "A pure javascript collection of ITS Design System components for UQ Library apps",
  "main": "src/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build:development": "cross-env NODE_ENV=development webpack && npm run compilesass:development",
    "build:production": "cross-env NODE_ENV=production webpack && npm run compilesass",
    "build:staging": "cross-env NODE_ENV=staging webpack && npm run compilesass",
    "build": "cross-env NODE_ENV=local webpack && npm run compilesass",
    "clean": "rm -rf node_modules dist cypress/screenshots cypress/video coverage .nyc_output",
    "codestyles:fix:all": "cross-env prettier-eslint '**/*.html' '**/*.scss' '**/*.css' '**/*.md' '**/*.js' '**/*.jsx' --write",
    "codestyles:fix:staged": "FILES=$(git diff --cached --name-only --diff-filter=ACM | grep -E \".(html|scss|css|md|js|jsx)$\"); npx prettier-eslint $FILES --write",
    "codestyles:fix": "cross-env prettier-eslint --write",
    "compilesass:development": "node-sass ./src/applications --output-style compressed --output ./dist/$CI_BRANCH/applications",
    "compilesass": "node-sass ./src/applications --output ./dist/applications --output-style compressed",
    "cypress:cache:list": "cypress cache list",
    "cypress:info": "cypress info",
    "cypress:open": "cypress open",
    "cypress:run:record": "cypress run --record",
    "cypress:run": "cypress run",
    "cypress:verify": "cypress verify",
    "cypress:version": "cypress version",
    "cypress:dashboard": "cypress run --record --parallel --config video=true,numTestsKeptInMemory=1 --ci-build-id $CI_BUILD_NUMBER",
    "eslint:fix": "eslint '**/*.js' --quiet --fix",
    "eslint": "eslint '**/*.js' --quiet",
    "precypress:run": "rm -rf .nyc_output",
    "start-test-server": "cross-env NODE_ENV=local USE_MOCK=true webpack-dev-server",
    "start:mock:no-open": "cross-env NODE_ENV=local USE_MOCK=true FULL_PATH=http://localhost:8080 NODE_OPTIONS='--max-old-space-size=2048' webpack-dev-server",
    "start:mock": "cross-env NODE_ENV=local USE_MOCK=true FULL_PATH=http://localhost:8080 NODE_OPTIONS='--max-old-space-size=2048' webpack-dev-server --open",
    "start": "cross-env NODE_ENV=local webpack-dev-server --open",
    "test:e2e:dashboard": "start-server-and-test start:mock:no-open http-get://localhost:8080 cypress:dashboard",
    "test:jest": "cross-env NODE_ENV=test FULL_PATH=http://localhost jest src/",
    "test:local": "start-server-and-test start-test-server http://localhost:8080 cypress:open",
    "test": "start-server-and-test start-test-server http://localhost:8080 cypress:run:record"
  },
  "engines": {
    "npm": "^8",
    "node": ">=16.13.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/uqlibrary/reusable-webcomponents.git"
  },
  "author": "UQ Library",
  "license": "MIT",
  "homepage": "https://library.uq.edu.au",
  "devDependencies": {
    "@babel/preset-react": "^7.22.5",
    "@cypress/code-coverage": "^3.11.0",
    "axe-core": "^4.7.2",
    "babel-plugin-istanbul": "^6.1.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "clean-css-cli": "^5.6.2",
    "copy-webpack-plugin": "^11.0.0",
    "cypress": "^12.17.2",
    "cypress-axe": "^1.4.0",
    "cypress-plugin-tab": "^1.0.5",
    "cypress-wait-until": "^1.7.2",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^3.4.1",
    "file-loader": "^6.2.0",
    "istanbul-lib-coverage": "^3.2.0",
    "mini-css-extract-plugin": "^1.6.2",
    "mocha": "^10.2.0",
    "node-sass": "^9.0.0",
    "nyc": "^15.1.0",
    "prettier": "^2.8.8",
    "prettier-eslint-cli": "^7.1.0",
    "rename-output-webpack-plugin": "^1.0.1",
    "sass-loader": "^13.3.2",
    "start-server-and-test": "^1.15.4",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "dependencies": {
    "@babel/core": "^7.22.9",
    "@babel/eslint-parser": "^7.22.9",
    "@babel/preset-env": "^7.22.9",
    "autoprefixer": "^10.4.14",
    "babel-loader": "^8.3.0",
    "content-replace-webpack-plugin": "^1.0.0",
    "core-js": "^3.31.1",
    "cross-env": "^7.0.3",
    "css-loader": "^4.3.0",
    "cssnano": "^6.0.1",
    "fetch-jsonp": "^1.3.0",
    "html-webpack-plugin": "^5.5.3",
    "js-cookie": "^3.0.5",
    "npm": "^9.8.1",
    "postcss-loader": "^7.3.3",
    "regenerator-runtime": "^0.13.11",
    "replace-in-file-webpack-plugin": "^1.0.6",
    "style-loader": "^3.3.3",
    "terser-webpack-plugin": "^5.3.9",
    "throttle-debounce": "^5.0.0",
    "validator": "^13.9.0"
  },
  "browserslist": [
    "defaults",
    "not IE 11"
  ],
  "nyc": {
    "all": false,
    "check-coverage": true,
    "branches": 100,
    "lines": 100,
    "functions": 100,
    "statements": 100,
    "include": [
      "**/src/**"
    ],
    "exclude": [
      "**/*.locale.js",
      "**/src/locale/**",
      "**/mock/**",
      "**/codebuild/**"
    ]
  }
}
