{
  "name": "reusable-webcomponents",
  "version": "1.0.0",
  "description": "A pure javascript collection of ITS Design System components for UQ Library apps",
  "main": "src/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "cross-env NODE_ENV=local webpack",
    "build:development": "cross-env NODE_ENV=development webpack",
    "build:staging": "cross-env NODE_ENV=staging webpack",
    "build:production": "cross-env NODE_ENV=production webpack",
    "start:mock": "cross-env NODE_ENV=local USE_MOCK=true FULL_PATH=http://localhost:8080 NODE_OPTIONS='--max-old-space-size=2048' webpack-dev-server",
    "start": "cross-env NODE_ENV=local webpack-dev-server --open",
    "clean": "rm -rf node_modules dist cypress/screenshots cypress/video",
    "start-test-server": "cross-env NODE_ENV=local webpack-dev-server",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "test": "start-server-and-test start-test-server http://localhost:8080 cypress:run"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/uqlibrary/reusable-webcomponents.git"
  },
  "author": "UQ Library",
  "license": "MIT",
  "homepage": "https://library.uq.edu.au",
  "devDependencies": {
    "@babel/core": "^7.5.0",
    "@babel/preset-env": "^7.13.9",
    "autoprefixer": "^9.6.0",
    "axe-core": "^4.1.2",
    "babel-loader": "^8.0.6",
    "copy-webpack-plugin": "^6.2.1",
    "cross-env": "^7.0.3",
    "css-loader": "^3.0.0",
    "cssnano": "^4.1.10",
    "cypress": "^6.5.0",
    "cypress-axe": "^0.12.2",
    "html-webpack-plugin": "^3.2.0",
    "node-sass": "^4.12.0",
    "postcss-loader": "^3.0.0",
    "replace-in-file-webpack-plugin": "^1.0.6",
    "sass-loader": "^7.1.0",
    "start-server-and-test": "^1.12.0",
    "style-loader": "^0.23.1",
    "terser-webpack-plugin": "^4.2.3",
    "webpack": "^4.35.2",
    "webpack-cli": "^3.3.5",
    "webpack-dev-server": "^3.7.2"
  },
  "dependencies": {
    "content-replace-webpack-plugin": "^1.0.0",
    "core-js": "^3.9.1",
    "js-cookie": "^2.2.1",
    "query-string": "^6.14.1",
